<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/javaee" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" id="WebApp_ID" version="2.5">
  <display-name>Bank_Application</display-name>
  <welcome-file-list>
    <welcome-file>index.html</welcome-file>
    <welcome-file>index.htm</welcome-file>
    <welcome-file>index.jsp</welcome-file>
    <welcome-file>default.html</welcome-file>
    <welcome-file>default.htm</welcome-file>
    <welcome-file>default.jsp</welcome-file>
  </welcome-file-list>
  <servlet>
    <description></description>
    <display-name>Bank_SignUp</display-name>
    <servlet-name>Bank_SignUp</servlet-name>
    <servlet-class>Controller.Bank_SignUp</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>Bank_SignUp</servlet-name>
    <url-pattern>/Bank_SignUp</url-pattern>
  </servlet-mapping>
  <servlet>
    <description></description>
    <display-name>Bank_Login</display-name>
    <servlet-name>Bank_Login</servlet-name>
    <servlet-class>Controller.Bank_Login</servlet-class>
    <init-param>
    <param-name>email</param-name>
	<param-value>Harshil@gmail.com</param-value>
    </init-param>
    <init-param>
    <param-name>Password</param-name>
    <param-value>super</param-value>
    </init-param>
  </servlet>
  <servlet-mapping>
    <servlet-name>Bank_Login</servlet-name>
    <url-pattern>/Bank_Login</url-pattern>
  </servlet-mapping>
</web-app>